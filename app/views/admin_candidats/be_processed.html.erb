	    			</div>
	    			<div class="atrt-date">
	    				Date d'entrée :
              <br>
              <%= pThWa.offre_job.date_poste %>
	    			</div>
	    			<div class="atrt-date">
	    				Date limite :
              <br>
              <%= pThWa.time_trying %>
	    			</div>
          </div>
	    			<div class="atrt-btn-gr">
		    			<a class="col-lg-3 col-xs-12 col-md-4 col-sm-12" href="<%= cbp_promise_no_validate_path(pThWa.cadre.cadre_info.id) %>">GERER</a><!-- promise to hire -->
	    			</div>
		    	</div>
	    	<% end %>
